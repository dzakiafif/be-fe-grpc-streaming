{
  "name": "@be-fe-grpc-streaming/frontend",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@grpc/grpc-js": "^1.12.0",
    "@grpc/proto-loader": "^0.7.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "lucide-react": "^0.575.0",
    "next": "~16.0.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/next:build",
        "outputs": [
          "{options.outputPath}"
        ],
        "defaultConfiguration": "production",
        "options": {
          "outputPath": "apps/frontend/dist"
        },
        "configurations": {
          "development": {
            "outputPath": "apps/frontend/.next"
          },
          "production": {
            "outputPath": "apps/frontend/dist"
          }
        }
      },
      "serve": {
        "executor": "@nx/next:server",
        "defaultConfiguration": "development",
        "options": {
          "buildTarget": "@be-fe-grpc-streaming/frontend:build",
          "dev": true
        },
        "configurations": {
          "development": {
            "buildTarget": "@be-fe-grpc-streaming/frontend:build:development",
            "dev": true
          },
          "production": {
            "buildTarget": "@be-fe-grpc-streaming/frontend:build:production",
            "dev": false
          }
        }
      },
      "export": {
        "executor": "@nx/next:export",
        "options": {
          "buildTarget": "@be-fe-grpc-streaming/frontend:build:production"
        }
      }
    }
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.0",
    "autoprefixer": "^10.4.24",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "postcss": "^8.5.6",
    "tailwind-merge": "^3.5.0",
    "tailwindcss": "^4.2.0",
    "tailwindcss-animate": "^1.0.7"
  }
}
